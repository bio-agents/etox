{% extends '::frontend.html.twig' %}
{% block title %}Listing Evidencesss {% endblock %}
{% block article %}
    {% include 'FrontendBundle:Search_keyword:tags_code.html.twig' %}
    {% set agenttipCounter=1 %}
    {% set stringStickyAgenttip = "" %}
    <div id="evidences">
        {% if resultSetDocuments | length != 0 %}
            <div id="documentsResults">
                <a name="documents"></a>
                <h3>Ranked sentences in Documents for "<span class="underline">{{ keyword }}</span>" keyword search</h3>
                <div class="btmspc-dbl">
                    <small>
                        <em>
                            Entity mentions are highlighted as follows:
                            <mark class="termSearched">What you searched</mark>, <mark class="compound">Compounds</mark>, <mark class="cytochrome">Cytochromes</mark>, <mark class="marker">Markers</mark>, <mark class="term">Terms</mark>, <mark class="specie">Species</mark>
                        </em>
                        . Curated evidences are indicated by:
                        <a class="curated" href="#"> </a>
                    </small>
                </div>

                <div class="results-summary">
                    {% set totalHits = resultSetDocuments.getTotalHits %}
                    {% set scoreToShow = orderBy | getScoreToShow %}
                    {% set arrayResultSetDocuments= resultSetDocuments.getResults %}
                    {% set order =  orderBy | getOrderToSource %}
                    <table>
                        <tr>
                            <td><strong>Total number of mentions displayed:</strong> {{ hitsShowed }}</td>
                            <td><strong>Maximum {{scoreToShow}} score:</strong> {{ arrayResultSetDocuments[0].getSource[order] |number_format(2) }}</td>
                            <td><strong>Minimum {{scoreToShow}} score:</strong> {% set last_index = resultSetDocuments | length -1 %}{{ resultSetDocuments[last_index].getSource[order] |number_format(2) }}</td>
                            <td><strong>{{scoreToShow}} mean:</strong> {{ meanScore | number_format(2) }}</td>
                            <td><strong>{{scoreToShow}} median:</strong> {{ medianScore | number_format(2) }}</td>
                        </tr>
                        {% if totalHits > 150  and whatToSearch!="withCytochromes" and whatToSearch!="withMarkers" and whatToSearch!="withCompounds"%}
                            <tr>
                                {% set totalHits = resultSetDocuments.getTotalHits %}
                                <td colspan="5"><strong>Total mentions:</strong> {{ totalHits }}. (All above scores are for the {{hitsShowed}} displayed hits). <br/><!-- Click here to display all results--></td>
                            </tr>
                        {% endif %}
                        <tr>
                            <td colspan="5">
                                <a href="{{ path('elasticSearch_keyword_orderby', {'whatToSearch': whatToSearch, 'source': source, 'keyword': entityName, 'orderBy': orderBy})}}?download=csv" class="start-preload"><strong>Download results</strong></a>
                                {# Uncomment this to enable multiple download formats. Just add new block for new formats
                                    <span data-agenttip="stickyDownload">
                                        <a href="#"><strong>Download results</strong></a>
                                    </span>
                                    {%
                                    set agenttip =  "
                                        <div id='stickyDownload' class='atip'>
                                            <ul class='download-list'>
                                                <li><a  href="~path('elasticSearch_keyword_orderby', {'whatToSearch': whatToSearch, 'source': source, 'keyword': entityName, 'orderBy': orderBy }) ~'?download=csv'~">
                                                        <img src="~ asset('images/icons/csv.png')~" border='0' />
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>"
                                    %}
                                    {% set stringStickyAgenttip = stringStickyAgenttip ~ agenttip  %}
                                #}
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="clear-both"></div>
                <small>
                    <em>
                        <span class="left">Color key for scores:&nbsp;&nbsp; </span>{% include 'FrontendBundle:Search_document:score_color_coding.html.twig' %}
                    </em>
                </small>
                <table id="tableResults" class="tablesorter">
                <thead>
                    <th><span data-agenttip class="has-tip" title="{{ mouseover_source}}">Source</span></th>
                    {% set currentPath = app.request.attributes.get('_route') %}
                    {% if currentPath=="elasticSearch_keyword" or currentPath=="elasticSearch_keyword_orderby" %}
                        <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'hepval'}) }}" {% if orderBy == "hepval"%} class="active"{% endif %} ><span data-agenttip class="has-tip" title="{{ mouseover_svm}}">SVM</span></a></th>
                        <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'svmConfidence'}) }}" {% if orderBy == "svmConfidence"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_conf}}">Conf.</span></a></th>
                        <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'pattern'}) }}" {% if orderBy == "pattern"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_pattern}}">Pattern</span></a></th>
                        <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'term'}) }}" {% if orderBy == "term"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_term}}">Term</span></a></th>
                        <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'rule'}) }}" {% if orderBy == "rule"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_rule}}">Rule</span></a></th>
                        {% if source == "abstract" %}
                            <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'toxicology'}) }}" {% if orderBy == "toxicology"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_toxicology}}">Toxicology</span></a></th>
                            <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'biomarker'}) }}" {% if orderBy == "biomarker"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_biomarker}}">Biomarker</span></a></th>
                        {% endif %}

                    {% elseif currentPath=="search_interface_search_field_whatToSearch_entityType_source_entity" or currentPath=="search_interface_search_field_whatToSearch_entityType_source_entity_orderby"%}

                        <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'hepval'}) }}" {% if orderBy == "hepval"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_svm}}">SVM</span></a></th>
                        <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'svmConfidence'}) }}" {% if orderBy == "svmConfidence"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_conf}}">Conf.</span></a></th>
                        <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'pattern'}) }}" {% if orderBy == "pattern"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_pattern}}">Pattern</span></a></th>
                        <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'term'}) }}" {% if orderBy == "term"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_term}}">Term</span></a></th>
                        <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'rule'}) }}" {% if orderBy == "rule"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_rule}}">Rule</span></a></th>
                         {% if source == "abstract" %}
                            <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'toxicology'}) }}" {% if orderBy == "toxicology"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_toxicology}}">Toxicology</span></a></th>
                            <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'biomarker'}) }}" {% if orderBy == "biomarker"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_biomarker}}">Biomarker</span></a></th>
                        {% endif %}

                    {% endif %}
                    <th>Sentence</th>
                </thead>
                <tbody>
                    {% set contador = 1 %}
                    {% for result in arrayResultsDoc %}
                        <tr class="document {{ cycle(['odd', 'even'], contador) }}">
                            {% set contador = contador + 1 %}
                            {% set className = result.getType %}
                            {% set text=result.getSource['text'] %}
                            {% set score = result.getSource['hepval'] | number_format(2)%}
                            {% set score = score|colorCodingScore %}
                            {% set patternCount = result.getSource['patternCount'] %}
                            {% set patternCount = patternCount|colorCodingScore %}
                            {% set ruleScore = result.getSource['ruleScore'] %}
                            {% set ruleScore = ruleScore | number_format(2)%}
                            {% set ruleScore = ruleScore|colorCodingScore %}
                            {% set termScore = result.getSource['hepTermVarScore'] %}
                            {% set termScore = termScore|colorCodingScore %}
                            {% set svmConfidence = result.getSource['svmConfidence'] %}
                            {% set svmConfidence = svmConfidence | number_format(2)%}
                            {% set svmConfidence = svmConfidence|colorCodingScore %}
                            {% if className == 'abstracts' %}
                                {{ text | highlightEntitiesAbstracts (result.getSource['text'], keyword, field, whatToSearch, source, entityType)| raw }}
                                {% set pmid=result.getSource['pmid'] %}
                                {% set link="http://www.ncbi.nlm.nih.gov/pubmed/" ~ pmid %}
                                {% set imageRoute = 'images/pubmed.png' %}
                                {% set score = result.getSource['hepval'] | number_format(2)%}
                            {% elseif className == 'documents' or className == 'documentswithcompounds' or className == 'documentswithcytochromes' or className == 'documentswithmarkers'%}
                                {% set kind = result.getSource['kind'] %}
                                {% if kind == "pubmed" %}
                                    {% set pmid=result.getSource['uid'] %}
                                    {% set sid=result.getSource['sentenceId'] %}
                                    {% set link="http://www.ncbi.nlm.nih.gov/pubmed/" ~ pmid %}
                                    {% set imageRoute = 'images/pubmed.png' %}
                                    <td class="center">
                                    <span data-agenttip='sticky{{ agenttipCounter }}'>
                                        <a href="{{ link }}" target="_blank"><img src="{{ asset(imageRoute) }}" class="outlinkLogo"/></a>
                                    </span>
                                    </td>
                                    {% set agenttip =  "<div id='sticky"~agenttipCounter~"' class='atip' >Pubmed link: <a href='"~link ~"'>"~link~"</a><br>Pubmed Id: "~ pmid ~"<br/>Sentence Id: "~ sid ~"<br/></div>" %}
                                    {% set agenttipCounter = agenttipCounter + 1 %}
                                    {% set stringStickyAgenttip = stringStickyAgenttip ~ agenttip  %}
                                {% elseif kind == "fulltext" %}
                                    {% set pmid=result.getSource['uid'] %}
                                    {% set sid=result.getSource['sentenceId'] %}
                                    {% set arrayPmid = pmid|split('_') %}
                                    {% set pmidCut = arrayPmid[1] %}
                                    {% set link="http://www.ncbi.nlm.nih.gov/pubmed/" ~ pmidCut %}
                                    {% set imageRoute = 'images/fulltext.png' %}
                                    <td class="center">
                                        <span data-agenttip='sticky{{ agenttipCounter }}'>
                                            <a href="{{ link }}" target="_blank"><img src="{{ asset(imageRoute) }}" class="outlinkLogo"/></a>
                                        </span>
                                    </td>
                                    {% set agenttip =  "<div id='sticky"~agenttipCounter~"' class='atip' >Pubmed link: <a href='"~link ~"'>"~link~"</a><br>Pubmed Id: "~ pmidCut ~"<br/>Sentence Id: "~ sid ~"</div>" %}
                                    {% set agenttipCounter = agenttipCounter + 1 %}
                                    {% set stringStickyAgenttip = stringStickyAgenttip ~ agenttip  %}
                                {% elseif kind == "nda" %}
                                    {% set link="http://www.accessdata.fda.gov/scripts/cder/drugsatfda/index.cfm" %}
                                    {% set sid=result.getSource['sentenceId'] %}
                                    {% set imageRoute = 'images/nda.png' %}
                                    <td class="center">
                                        <span data-agenttip='sticky{{ agenttipCounter }}'>
                                            <a href="{{ link }}" target="_blank"><img src="{{ asset(imageRoute) }}" class="outlinkLogo"/ alt="FDA New Drug Approvals (NDAs)" title="FDA New Drug Approvals (NDAs)"></a>
                                        </span>
                                    </td>
                                    {% set agenttip =  "<div id='sticky"~agenttipCounter~"' class='atip' >Sentence Id: "~ sid ~"</div>" %}
                                    {% set agenttipCounter = agenttipCounter + 1 %}
                                    {% set stringStickyAgenttip = stringStickyAgenttip ~ agenttip  %}
                                {% elseif kind == "epar" %}
                                    {% set link="http://www.ema.europa.eu/ema/index.jsp?curl=pages/medicines/landing/epar_search.jsp" %}
                                    {% set imageRoute = 'images/epar.png' %}
                                    {% set sid=result.getSource['sentenceId'] %}
                                    <td class="center">
                                        <span data-agenttip='sticky{{ agenttipCounter }}'>
                                            <a href="{{ link }}" target="_blank"><img src="{{ asset(imageRoute) }}" class="outlinkLogo"/ alt="European Public Assessment Reports (EPARs)" title="European Public Assessment Reports (EPARs)"></a>
                                        </span>
                                    </td>
                                    {% set agenttip =  "<div id='sticky"~agenttipCounter~"' class='atip' >Sentence Id: "~ sid ~"</div>" %}
                                    {% set agenttipCounter = agenttipCounter + 1 %}
                                    {% set stringStickyAgenttip = stringStickyAgenttip ~ agenttip  %}
                                {% endif %}
                                {% set arrayText= text | highlightEntitiesDocumentsKeywords (result.getSource['sentenceId'],keyword,field,whatToSearch, source, entityType, agenttipCounter) %}
                                {% set text = arrayText[0] | raw %}
                                {% set stringStickyAgenttip = stringStickyAgenttip ~ arrayText[1] %}
                                {% set agenttipCounter = arrayText[2] %}
                            {% endif%}
                            <td class="center">
                                {{ score  | raw}}
                            </td>
                            <td class="center">
                                {{ svmConfidence  | raw}}
                            </td>
                            <td class="center">
                                {{ patternCount  | raw}}
                            </td>
                            <td class="center">
                                {{ termScore  | raw}}
                            </td>
                            <td class="center">
                                {{ ruleScore  | raw}}
                            </td>
                            <td>
                                {{ text | raw }}
                            </td>
                        </tr>
                {% endfor %}
                </tbody>
            </table>
            <!-- here pagination block for documents  -->
                {% set currentPath = app.request.attributes.get('_route') %}
                {% if currentPath == "elasticSearch_keyword" or currentPath=="elasticSearch_keyword_orderby" %}
                    <div class="pagination">
                        {{ simple_paginator_render(
                            'elasticSearch_keyword_orderby', 'documents',
                                {
                                    'routeParams' : {'keyword' : keyword, 'whatToSearch' : whatToSearch, 'source' : source, 'orderBy' : orderBy},
                                    'container_class' : 'pagination'
                                }
                            )
                        }}
                    </div>
                {% elseif currentPath=="search_interface_search_field_whatToSearch_entityType_source_entity" or currentPath=="search_interface_search_field_whatToSearch_entityType_source_entity_orderby" %}
                    <div class="pagination">
                        {{ simple_paginator_render(
                            'search_interface_search_field_whatToSearch_entityType_source_entity_orderby', 'documents',
                                {
                                    'routeParams' : {'field' : field, 'entityType' : entityType, 'entityName' : keyword, 'whatToSearch' : whatToSearch, 'source' : source, 'orderBy' : orderBy},
                                    'container_class' : 'pagination'
                                }
                            )
                        }}
                    </div>
                {% endif %}
            </div>
        {% endif %}
        {% if resultSetAbstracts | length != 0 %}
            {% if resultSetAbstracts.getTotalHits != 0 %}
                <div id="abstractsResults">
                    <a name="abstracts"></a>
                    <h3>Ranked Abstracts for "<span class="underline">{{ keyword }}</span>" keyword search</h3>
                    <div data-alert class="alert-box info radius">
                          Entity mentions <strong>ARE NOT highlighted</strong> when searching <strong>inside Abstracts</strong>
                          <a href="#" class="close">&times;</a>
                    </div>
                     <div class="results-summary">
                    {% set totalHits = resultSetAbstracts.getTotalHits %}
                    {% set scoreToShow = orderBy | getScoreToShow %}
                    <table>
                        <tr>
                            <td><strong>Total number of mentions displayed:</strong> {{ hitsShowed }}</td>
                            <td><strong>Maximum {{scoreToShow}} score:</strong> {{ resultSetAbstracts[0].getSource[orderBy] |number_format(2) }}</td>
                            <td><strong>Minimum {{scoreToShow}} score:</strong> {% set last_index = resultSetAbstracts | length -1 %}{{ resultSetAbstracts[last_index].getSource[orderBy] |number_format(2) }}</td>
                            <td><strong>{{scoreToShow}} mean:</strong> {{ meanScore | number_format(2) }}</td>
                            <td><strong>{{scoreToShow}} median:</strong> {{ medianScore | number_format(2) }}</td>
                        </tr>
                        {% if totalHits > 150 %}
                            <tr>
                                {% set totalHits = resultSetAbstracts.getTotalHits %}
                                <td colspan="5"><strong>Total mentions:</strong> {{ totalHits }}. (All above scores are for the {{hitsShowed}} displayed hits). <!-- <br/>Click here to display all results --></td>
                            </tr>
                        {% endif %}
                        <tr>
                            <td colspan="5">
                                <a href="{{ path('elasticSearch_keyword_orderby', {'whatToSearch': whatToSearch, 'source': source, 'keyword': entityName, 'orderBy': orderBy})}}?download=csv" class="start-preload"><strong>Download results</strong></a>
                                {# Uncomment this to enable multiple download formats. Just add new block for new formats
                                    <span data-agenttip="stickyDownload">
                                        <a href="#"><strong>Download results</strong></a>
                                    </span>
                                    {%
                                    set agenttip =  "
                                        <div id='stickyDownload' class='atip'>
                                            <ul class='download-list'>
                                                <li><a  href="~path('elasticSearch_keyword_orderby', {'whatToSearch': whatToSearch, 'source': source, 'keyword': entityName, 'orderBy': orderBy }) ~'?download=csv'~">
                                                        <img src="~ asset('images/icons/csv.png')~" border='0' />
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>"
                                    %}
                                    {% set stringStickyAgenttip = stringStickyAgenttip ~ agenttip  %}
                                #}
                            </td>
                        </tr>
                    </table>
                </div>
                    <div class="clear-both"></div>
                    <small>
                        <em>
                            <span class="left">Color key for scores:&nbsp;&nbsp; </span>{% include 'FrontendBundle:Search_document:score_color_coding.html.twig' %}
                        </em>
                    </small>
                    <table id="tableResults" class="tablesorter">
                        <thead>
                            <th><span data-agenttip class="has-tip" title="{{ mouseover_source}}">Source</span></th>
                            {% set currentPath = app.request.attributes.get('_route') %}
                            {% if currentPath=="elasticSearch_keyword" or currentPath=="elasticSearch_keyword_orderby" %}
                                <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'hepval'}) }}" {% if orderBy == "hepval"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_svm}}">SVM</span></a></th>
                                <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'svmConfidence'}) }}" {% if orderBy == "svmConfidence"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_conf}}">Conf.</span></a></th>
                                <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'pattern'}) }}" {% if orderBy == "pattern"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_pattern}}">Pattern</span></a></th>
                                <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'term'}) }}" {% if orderBy == "term"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_term}}">Term</span></a></th>
                                <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'rule'}) }}" {% if orderBy == "rule"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_rule}}">Rule</span></a></th>
                                <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'toxicology'}) }}" {% if orderBy == "toxicology"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_toxicology}}">Toxicology</span></a></th>
                                <th><a href="{{ path('elasticSearch_keyword_orderby',{'whatToSearch': whatToSearch, 'source': source, 'keyword': keyword, 'orderBy': 'biomarker'}) }}" {% if orderBy == "biomarker"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_biomarker}}">Biomarker</span></a></th>
                            {% elseif currentPath=="search_interface_search_field_whatToSearch_entityType_source_entity" or currentPath=="search_interface_search_field_whatToSearch_entityType_source_entity_orderby"%}

                                <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'hepval'}) }}" {% if orderBy == "hepval"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_svm}}">SVM</span></a></th>
                                <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'svmConfidence'}) }}" {% if orderBy == "svmConfidence"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_conf}}">Conf.</span></a></th>
                                <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'pattern'}) }}" {% if orderBy == "pattern"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_pattern}}">Pattern</span></a></th>
                                <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'term'}) }}" {% if orderBy == "term"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_term}}">Term</span></a></th>
                                <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'rule'}) }}" {% if orderBy == "rule"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_rule}}">Rule</span></a></th>
                                <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'toxicology'}) }}" {% if orderBy == "toxicology"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_toxicology}}">Toxicology</span></a></th>
                                <th><a href="{{ path('search_interface_search_field_whatToSearch_entityType_source_entity_orderby',{'field': field, 'whatToSearch': whatToSearch, 'entityType': entityType, 'source': source, 'entityName': entityName, 'orderBy': 'biomarker'}) }}" {% if orderBy == "biomarker"%} class="active"{% endif %}><span data-agenttip class="has-tip" title="{{ mouseover_biomarker}}">Biomarker</span></a></th>

                            {% endif %}
                            <th>Sentence</th>
                        </thead>
                        <tbody>
                            {% set contador = 1 %}
                            {% for result in arrayResultsAbs %}
                                <tr class="document {{ cycle(['odd', 'even'], contador) }}">
                                    {% set contador = contador + 1 %}
                                    {% set className = result.getType %}
                                    {% set text=result.getSource['text'] %}
                                    {% set score = result.getSource['hepval'] | number_format(2)%}
                                    {% set score = score|colorCodingScore %}
                                    {% if className == "abstracts" or className == "abstractswithcompounds" %}
                                        {% set arrayText= text | highlightEntitiesAbstracts (result,keyword,field,whatToSearch, source, entityType, agenttipCounter) %}
                                        {% set text = arrayText[0] | raw %}
                                        {% set stringStickyAgenttip = stringStickyAgenttip ~ arrayText[1] %}
                                        {% set agenttipCounter = arrayText[2] %}
                                        {% set pmid=result.getSource['pmid'] %}
                                        {% set link="http://www.ncbi.nlm.nih.gov/pubmed/" ~ pmid %}
                                        {% set imageRoute = 'images/pubmed.png' %}
                                        {% set patternCount = result.getSource['patternCount'] %}
                                        {% set patternCount = patternCount|colorCodingScore %}
                                        {% set ruleScore = result.getSource['ruleScore'] %}
                                        {% set ruleScore = ruleScore | number_format(2)%}
                                        {% set ruleScore = ruleScore|colorCodingScore %}
                                        {% set termScore = result.getSource['hepTermVarScore'] %}
                                        {% set termScore = termScore|colorCodingScore %}
                                        {% set svmConfidence = result.getSource['svmConfidence'] %}
                                        {% set svmConfidence = svmConfidence | number_format(2)%}
                                        {% set svmConfidence = svmConfidence|colorCodingScore %}
                                        {% set toxicology = result.getSource['toxicology'] %}
                                        {% set toxicology = toxicology | number_format(2)%}
                                        {% set toxicology = toxicology | colorCodingScore %}
                                        {% set biomarker = result.getSource['biomarker'] %}
                                        {% set biomarker = biomarker | number_format(2)%}
                                        {% set biomarker = biomarker | colorCodingScore %}

                                    {% elseif className == 'Document' or className == 'DocumentWithCompound' or className == 'DocumentWithCytochrome' or className == 'DocumentWithMarker'%}
                                        {% set kind = result.getKind %}
                                        {% if kind == "pubmed" %}
                                            {% set pmid=result.getUid %}
                                            {% set link="http://www.ncbi.nlm.nih.gov/pubmed/" ~ pmid %}
                                            {% set imageRoute = 'images/pubmed.png' %}
                                        {% elseif kind == "fulltext" %}
                                            {% set pmid=result.getUid %}
                                            {% set arrayPmid = pmid|split('_') %}
                                            {% set pmidCut = arrayPmid[1] %}
                                            {% set link="http://www.ncbi.nlm.nih.gov/pubmed/" ~ pmidCut %}
                                            {% set imageRoute = 'images/fulltext.png' %}
                                        {% elseif kind == "nda" %}
                                            {% set link="http://www.fda.gov" %}
                                            {% set imageRoute = 'images/nda.png' %}
                                        {% elseif kind == "epar" %}
                                            {% set link="http://www.fda.gov" %}
                                            {% set imageRoute = 'images/epar.png' %}
                                        {% endif %}
                                        {% set arrayText= text | highlightEntitiesDocuments (result,keyword,field,whatToSearch, source, entityType, agenttipCounter) %}
                                        {% set text = arrayText[0] | raw %}
                                        {% set stringStickyAgenttip = stringStickyAgenttip ~ arrayText[1] %}
                                        {% set agenttipCounter = arrayText[2] %}
                                    {% endif%}
                                    <td class="center">
                                        <a href="{{ link }}" target="_blank"><img src="{{ asset(imageRoute) }}" class="outlinkLogo"/></a>
                                    </td>
                                    <td class="center">
                                        {{ score  | raw}}
                                    </td>
                                    <td>
                                        {{ svmConfidence | raw }}
                                    </td>
                                    <td>
                                        {{ patternCount | raw }}
                                    </td>
                                    <td>
                                        {{ termScore | raw }}
                                    </td>
                                    <td>
                                        {{ ruleScore | raw }}
                                    </td>
                                    <td>
                                        {{ toxicology | raw }}
                                    </td>
                                    <td>
                                        {{ biomarker | raw }}
                                    </td>
                                    <td>
                                        {{ text | raw }}
                                    </td>
                                </tr>
                        {% endfor %}
                        </tbody>
                    </table>
                    <!-- here pagination block for abstracts -->
                    <div class="pagination">
                        {{ simple_paginator_render(
                            'elasticSearch_keyword_orderby', 'abstracts',
                                {
                                    'routeParams' : {'keyword' : keyword, 'whatToSearch' : whatToSearch, 'source' : source, 'orderBy' : orderBy},
                                    'container_class' : 'pagination'
                                }
                            )
                        }}
                    </div>
                </div>
            {% endif %}
        {% endif %}
        {# Here we rebuild the stringStickyAgenttip inside its container divs #}
        <div id="mystickyagenttip" class="stickyagenttip">
            <div style="padding:5px">
                {{ stringStickyAgenttip | raw }}
            </div>
            <div class="stickystatus"></div>
        </div>
    {% include 'FrontendBundle:Search_document:scripts_code.html.twig' %}
{% endblock %}